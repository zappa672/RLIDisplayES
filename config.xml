  <layouts>
    <layout size="1280x1024">
      <rli-circle x="512" y="512" radius="480" font="F8X8B" />
      <menu x="-4" y="152" width="236" height="262" font="F12X14B" />
      <magnifier x="-10" y="155" width="224" height="224" />

      <panels>
        <value-bar name="gain" x="4" y="4" width="151" height="21" bar_width="50" font="F12X14B" />
        <value-bar name="water" x="4" y="29" width="151" height="21" bar_width="50" font="F12X14B" />
        <value-bar name="rain" x="4" y="54" width="151" height="21" bar_width="50" font="F12X14B" />
        <value-bar name="apch" x="4" y="79" width="151" height="21" bar_width="50" font="F12X14B" />
        <value-bar name="emission" x="159" y="4" width="163" height="21" bar_width="50" font="F12X14B" />

        <label name="label5" x="4" y="104" width="102" height="20" font="F12X14B" />
        <label name="band" x="4" y="128" width="102" height="20" font="F12X14B" />
        <label name="label1" x="-244" y="44" width="112" height="20" font="F12X14B" />
        <label name="label2" x="-244" y="68" width="112" height="20" font="F12X14B" />
        <label name="label3" x="-244" y="92" width="112" height="20" font="F12X14B" />
        <label name="label4" x="-244" y="-68" width="96" height="32" font="F16X28B" />

        <!-- rect="left_x_top_x_width_x_height" -->
