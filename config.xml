          <text name="units" rect="176x12x56x14" allign="center" font="F14X14B" />
          <rect name="splitter" rect="174x0x1x36" />
        </panel>

        <panel name="vn" x="4" y="-4" width="140" height="66">
          <text name="header" rect="0x6x132x14" allign="center" font="F12X14B" />
          <table name="table" top="28" row_height="18">
            <column left="4" width="36" allign="left" font="F12X14B" />
            <column left="40" width="60" allign="right" font="F12X14B" />
            <column left="100" width="36" allign="left" font="F8X14B" />
