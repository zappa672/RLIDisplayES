            <column left="100" width="40" allign="left" font="F8X14B" />
          </table>
        </panel>

        <panel name="course" x="-4" y="4" width="236" height="38">
          <table name="table" top="4" row_height="18">
            <column left="4" width="116" allign="left" font="F12X14B" />
            <column left="120" width="60" allign="right" font="F12X14B" />
            <column left="186" width="36" allign="left" font="F8X14B" />
          </table>
        </panel>

        <panel name="position" x="-4" y="46" width="236" height="38">
          <table name="table" top="4" row_height="18">
            <column left="4" width="116" allign="left" font="F12X14B" />
            <column left="120" width="112" allign="right" font="F12X14B" />
          </table>
        </panel>

        <panel name="blank" x="-4" y="88" width="236" height="38">
        </panel>

        <panel name="clock" x="-4" y="130" width="236" height="18">
          <text name="label" rect="4x4x80x14" allign="center" font="F12X14B" />
          <text name="time" rect="120x4x96x14" allign="center" font="F12X14B" />
        </panel>

        <panel name="danger" x="-4" y="-330" width="236" height="20">
          <text name="label" rect="0x0x236x20" allign="center" font="F12X14B" />
        </panel>

        <panel name="tails" x="-4" y="-306" width="236" height="20">
          <table name="table" top="3" row_height="18">
            <column left="4" width="116" allign="left" font="F12X14B" />
            <column left="120" width="60" allign="right" font="F12X14B" />
            <column left="186" width="36" allign="left" font="F8X14B" />
          </table>
        </panel>

        <panel name="danger-details" x="-4" y="-264" width="236" height="38">
          <table name="table" top="3" row_height="18">
            <column left="4" width="116" allign="left" font="F12X14B" />
            <column left="120" width="60" allign="right" font="F12X14B" />
            <column left="186" width="36" allign="left" font="F8X14B" />
          </table>
        </panel>

        <panel name="vector" x="-4" y="-240" width="236" height="20">
          <table name="table" top="3" row_height="18">
            <column left="4" width="116" allign="left" font="F12X14B" />
            <column left="120" width="60" allign="right" font="F12X14B" />
            <column left="186" width="36" allign="left" font="F8X14B" />
          </table>
        </panel>

        <panel name="targets" x="-4" y="-68" width="236" height="168">
          <rect name="header-middle" rect="118x0x1x20" />
          <rect name="header-bottom" rect="0x20x236x1" />

          <text name="header" rect="4x4x228x14" allign="left" font="F12X14B" />
          <text name="current-target" rect="4x4x110x14" allign="right" font="F12X14B" />
          <text name="target-count" rect="4x4x228x14" allign="right" font="F12X14B" />

          <table name="table" top="24" row_height="18">
            <column left="4" width="116" allign="left" font="F12X14B" />
            <column left="120" width="60" allign="right" font="F12X14B" />
            <column left="186" width="36" allign="left" font="F8X14B" />
          </table>
        </panel>

        <panel name="cursor" x="-4" y="-4" width="236" height="60">
